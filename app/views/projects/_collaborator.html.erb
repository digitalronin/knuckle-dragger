<div class="row collaborator-container">
  <div class="col">
    <div class="card collaborator">
      <%= link_to collaborator.html_url, target: :blank do %>
        <img class="card-img-top" src="<%= collaborator.avatar_url %>" alt="<%= collaborator.login %>">
      <% end %>
      <div class="card-body">
        <h5 class="card-title">
          <%= link_to collaborator.login, collaborator.html_url, target: :blank %>
        </h5>
      </div>
    </div>
  </div>

  <div class="col-7 assigned-issues">
    <%= render partial: "assigned_issue", collection: repo.issues_assigned_to(collaborator.login), locals: { repo: repo, collaborator: collaborator } %>
  </div>
</div>
