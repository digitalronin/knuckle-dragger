<div class="row contributor-container">
  <div class="col">
    <div class="card contributor">
      <%= link_to contributor.html_url, target: :blank do %>
        <img class="card-img-top" src="<%= contributor.avatar_url %>" alt="<%= contributor.login %>">
      <% end %>
      <div class="card-body">
        <h5 class="card-title">
          <%= link_to contributor.login, contributor.html_url, target: :blank %>
        </h5>
      </div>
    </div>
  </div>

  <div class="col-7 assigned-issues">
    <%= render partial: "assigned_issue", collection: repo.issues_assigned_to(contributor.login) %>
  </div>
</div>
